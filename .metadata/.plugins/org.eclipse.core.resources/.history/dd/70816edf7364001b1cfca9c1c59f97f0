package esercizio_base_2;

public class Articolo {
	
	int id;
	String codice;
	String descrizione;
	int quantita;
	
	public void get() {
		
	}
	
    public void set() {
		
	}
    
    @Override
	public String toString(){
		return "Articolo: "+id+"; Codice: "+codice+"; Descrizione: "+descrizione+"; Quantità: "+quantità;
	}

}
